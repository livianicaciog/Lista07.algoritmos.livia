#include <iostream>

using namespace std;

int função(int x,int y){
    int maior=0;
 
    if (x>y){
       maior=x;
    }else if(x<y) {
        maior= y;
    }else{
        maior=0;
    }
    
    cout<<maior;
    return maior;
}   

int main()
{
    int x, y;
    cin>>x>>y;
    função(x,y);
    
    return 0;
}
